    "fs-extra": "7.0.1",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "jest-emotion": "10.0.6",
    "lint-staged": "8.1.0",
    "nodemon": "1.18.9",
    "prettier": "1.15.3",
    "raw-loader": "1.0.0",
    "react-test-renderer": "16.8.0-alpha.1",
    "url-loader": "1.1.2",
    "webpack-bundle-analyzer": "3.0.3"
  }
}
