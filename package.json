{
  "private": true,
  "name": "simon-kjellberg",
  "version": "0.0.0",
  "author": "Simon Kjellberg",
  "license": "MIT",
  "repository": "simonkberg/simon-kjellberg",
  "bugs": "https://github.com/simonkberg/simon-kjellberg/issues",
  "homepage": "https://github.com/simonkberg/simon-kjellberg",
  "engines": {
    "node": ">=7.0.0"
  },
  "scripts": {
    "build:dev": "NODE_ENV=development nyr build",
    "build:prod": "NODE_ENV=production nyr build",
    "build": "node ./bin/compile.js",
    "dev": "nyr build:dev && nyr start:dev",
    "format": "nyr prettier -- \"*.js\" \"{bin,server,src,webpack}/**/*.{js,css}\"",
    "lint:css:fix": "nyr lint:css -- --fix",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:fix": "nyr lint:css:fix && nyr lint:js:fix",
    "lint:js:fix": "nyr lint:js -- --fix",
    "lint:js": "eslint bin/** server src webpack",
    "lint": "nyr lint:css && nyr lint:js",
    "precommit": "lint-staged",
    "prettier": "prettier --single-quote --no-semi --trailing-comma es5 --write",
    "prod": "nyr build:prod && nyr start:prod",
    "ssl": "node ./bin/ssl.js",
    "start:dev": "NODE_ENV=development nyr start",
    "start:prod": "NODE_ENV=production nyr start",
    "start": "node ./bin/serve.js",
    "test": "nyr lint"
  },
  "lint-staged": {
    "*.js": [
      "nyr prettier",
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "prettier",
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@slack/client": "^3.11.0",
    "async-cache": "^1.1.0",
    "body-parser": "^1.17.2",
    "chalk": "^2.1.0",
    "classnames": "^2.2.4",
    "compression": "^1.7.0",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.8",
    "dotenv": "^4.0.0",
    "emoji-datasource": "^3.0.0",
    "express": "^4.15.4",
    "express-webpack-assets": "0.0.2",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "letiny": "^0.1.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "ms": "^2.0.0",
    "newrelic": "^1.40.0",
    "normalizr": "^3.2.3",
    "nyr": "^1.1.0",
    "prop-types": "^15.5.10",
    "raf": "^3.3.2",
    "react": "^16.0.0",
    "react-addons-css-transition-group": "^15.6.0",
    "react-dom": "^16.0.0",
    "react-ga": "^2.2.0",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "^3.0.0-beta",
    "react-motion": "^0.5.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.5",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "request": "^2.81.0",
    "reselect": "^3.0.1",
    "resize-observer-polyfill": "^1.4.2",
    "sanitize.css": "^5.0.0",
    "serialize-javascript": "^1.4.0",
    "serve-favicon": "^2.4.3",
    "ws": "^3.1.0"
  },
  "devDependencies": {
    "@simonkberg/eslint-config-react": "^10.4.0",
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.5",
    "del": "^3.0.0",
    "eslint": "^4.5.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-loader": "^1.9.0",
    "file-loader": "^1.1.0",
    "husky": "^0.14.3",
    "isomorphic-style-loader": "^3.0.0",
    "lint-staged": "^4.0.3",
    "postcss": "^6.0.9",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.6",
    "postcss-smart-import": "^0.7.5",
    "prettier": "^1.5.3",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2"
  }
}
