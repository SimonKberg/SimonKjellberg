{
  "name": "simon-kjellberg",
  "version": "1.0.0",
  "private": true,
  "description": "My personal website",
  "repository": "https://github.com/simonkberg/simon-kjellberg",
  "license": "MIT",
  "author": "Simon Kjellberg <simon.kjellberg@gmail.com> (https://simonkjellberg.com)",
  "scripts": {
    "build": "node scripts/build.js",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint . --fix",
    "prettier:check": "prettier --list-different '**/*.{js,json,md}'",
    "prettier:format": "prettier --write '**/*.{js,json,md,ya?ml}'",
    "start": "nodemon scripts/start.js",
    "serve": "node scripts/serve.js",
    "test": "flow check && yarn eslint:check && yarn prettier:check && jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "jest --findRelatedTests",
      "flow focus-check",
      "git add"
    ],
    "*.{json,md,ya?ml}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 ChromeAndroid versions",
    "last 2 Edge versions",
    "last 2 Firefox versions",
    "last 2 FirefoxAndroid versions",
    "last 2 iOS versions",
    "last 2 Opera versions",
    "last 2 Safari versions",
    "last 2 Samsung versions"
  ],
  "dependencies": {
    "@emotion/core": "10.0.6",
    "@emotion/styled": "10.0.6",
    "@slack/client": "4.8.0",
    "apollo-cache-inmemory": "1.4.2",
    "apollo-client": "2.4.11",
    "apollo-link": "1.2.6",
    "apollo-link-http": "1.5.9",
    "apollo-link-ws": "1.0.12",
    "apollo-server-express": "2.3.1",
    "apollo-utilities": "1.1.2",
    "common-tags": "1.8.0",
    "compression": "1.7.3",
    "cookie-session": "2.0.0-beta.3",
    "cross-fetch": "3.0.0",
    "dataloader": "1.4.0",
    "dotenv": "6.2.0",
    "emoji-datasource": "4.1.0",
    "emotion-theming": "10.0.6",
    "express": "4.16.4",
    "graphql": "14.1.1",
    "graphql-tag": "2.10.1",
    "helmet": "3.15.0",
    "http-status": "1.3.1",
    "invariant": "2.2.4",
    "ioredis": "4.3.0",
    "lru-cache": "5.1.1",
    "minimist": "1.2.0",
    "morgan": "1.9.1",
    "ms": "2.1.1",
    "newrelic": "5.1.0",
    "next": "7.0.2",
    "next-offline": "3.3.5",
    "opn": "5.4.0",
    "polished": "2.3.3",
    "prop-types": "15.6.2",
    "react": "16.8.0-alpha.1",
    "react-apollo": "2.3.3",
    "react-dom": "16.8.0-alpha.1",
    "react-spring": "7.2.9",
    "recompose": "0.30.0",
    "sanitize.css": "8.0.0",
    "screenfull": "4.0.0",
    "serve-favicon": "2.5.0",
    "styled-tools": "1.7.0",
    "subscriptions-transport-ws": "0.9.15"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.2.0",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/preset-flow": "7.0.0",
    "@packtracker/webpack-plugin": "1.1.2",
    "@simonkberg/eslint-config": "11.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-plugin-emotion": "10.0.6",
    "chokidar": "2.0.4",
    "eslint": "5.12.1",
