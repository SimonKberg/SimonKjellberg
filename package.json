  "homepage": "https://github.com/simonkberg/simon-kjellberg",
  "engines": {
    "node": ">=7.0.0"
  },
  "scripts": {
    "test": "nyr lint",
    "format": "prettier-eslint --write \"*.js\" \"{bin,server,src,webpack}/**/*.js\"",
    "lint": "nyr lint:stylelint && nyr lint:eslint",
    "lint:stylelint": "stylelint \"src/**/*.css\"",
    "lint:eslint": "eslint --fix bin/** server src webpack",
    "lint:staged": "lint-staged",
    "start": "node ./bin/serve.js",
    "start:prod": "NODE_ENV=production nyr start",
    "start:dev": "NODE_ENV=development nyr start",
    "build": "node ./bin/compile.js",
    "build:prod": "NODE_ENV=production nyr build",
    "build:dev": "NODE_ENV=development nyr build",
    "ssl": "node ./bin/ssl.js",
    "dev": "nyr build:dev && nyr start:dev",
    "prod": "nyr build:prod && nyr start:prod"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ],
    "*.css": [
      "stylelint"
    ]
  },
  "pre-commit": "lint:staged",
